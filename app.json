{
  "expo": {
    "name": "Filetül Barkod Sorgulama",
    "slug": "filetul",
    "version": "2.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Kartvizit yüklemesi için gereken galeri izni"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.filetul.barkod",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Barkod Taramak İçin Kamera İzni Vermeniz Gerekmektedir."
      }
    },
    "android": {
      "package": "com.filetul.barkod",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA_ROLL",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7a94678b-5159-4cd1-be66-abe7e0b4a5d7"
      }
    }
  }
}
